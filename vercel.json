{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/firebase-messaging-sw.js", "destination": "/firebase-messaging-sw.js" },
    {
      "source": "/(.*)\\.(?:js|css|png|jpg|jpeg|svg|ico|json|txt|webp|woff|woff2|map)",
      "destination": "/$1"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
